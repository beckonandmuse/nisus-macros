{\pard \s1879 {\par
{\cs2501 \cf1 # Takes note of current file name in order to save the current}\par
{\cs2501 \cf1 # file to a different folder with the same filename.}\par
\par
\tab $doc = Document.active\par
\tab
$path = $doc.filePath\par
\par
{\cs2501 \cf1 # Establishes current filename & Current immediate parent folder for file.}\par
\par
\tab $currentName = $path.lastFilePathComponent\par
\par
\tab
{\cs2501 \cf1 # For every parent folder within our path, there is an array index.}\par
\tab {\cs2501 \cf1 # The parent folders\rquote  names are the values for those indices.}\par
\tab $components = $path.filePathComponents\par
\par
\tab
{\cs2501 \cf1 # We only care to check the farthest subfolder.\par
\tab }$subfolderCount = $components.count\par
\tab {\cs2501 \cf1 # If there are three for count, index [2] is the last.\par
\tab # The last is always the filename with extension.\par
\tab # Therefore, we want the second to last, which is two lower\par
\tab # than the array.count.\par
\tab }$currentFolderIndex = $subfolderCount - 2{\cs2501 \cf1 \par
\tab }$currentFolder = $components[$currentFolderIndex]\par
\tab
\par
\tab {\cs2501 \cf1 # Establishes the new location and filename for the results of the\par
\tab # following macro to be saved.}\par
\tab $newFolder = \lquote ~/Documents/GHProjects/help-tips/separate-tables-pt-2/\rquote
\par
\tab $newfilePath = $newFolder.filePathByAppendingComponent($currentName)\par
\par
{\cs2501 \cf1 # Checks to see if the user is working on the proper file by making sure}\par
{\cs2501 \cf1 # that the file the script is currently being executed on is located in }\par
{\cs2501 \cf1 # ...GHProjects/help-tips/tables-separated... and then ...}\par
\par
\tab
If $currentFolder != \ldblquote separate-tables\rdblquote \par
\tab \tab $folder = $path.filePathByRemovingLastComponent\par
\tab \tab Prompt \ldblquote Sorry! Current folder is $folder !\rdblquote \par
\tab Else\par
\tab \tab \par
\tab \tab {\cs2501 \cf1 # Attempts to save a new copy of the file}\par
\par
\tab
\tab \tab $isSaved = Save As $newfilePath\par
}}{\pard \s2086 \nowidctlpar\li720\lin720\fi0\ri0\rin0\tx720 {\fs22\nisuslangi0 \tab \tab If ! $isSaved\par
\tab \tab \tab Prompt "Failed to save the document to $newfilePath !"\par
\tab \tab End\par
End\par
